<configuration>
  <appSettings/>
  <connectionStrings/>

  <system.webServer>
    <httpErrors errorMode="Detailed" />
    <asp scriptErrorSentToBrowser="true"/>
</system.webServer>
<system.web>
	<pages maintainScrollPositionOnPostBack="true" />
    <customErrors mode="Off"/>
    <compilation debug="true"/>
</system.web>
</configuration>